import java.util.ArrayList;
import java.util.Iterator;

public class listArrays {
	public static void main (String[] args) {
		

	ArrayList listTest = new ArrayList();
	
	listTest.add("first item");
	listTest.add("second item");
	listTest.add("third item");
	listTest.add(7);

	Iterator it = listTest.iterator();
	
	while(it.hasNext()) {
		System.out.println(it.next());
		
	}
	
	// Remove an item from the list
	listTest.remove("second item");
	
	// Print new list
	System.out.println("The Whole List is made up of " + listTest);
	
	//Get the item at index position 1
	System.out.println("Position 1 is " + listTest.get(1));
	
	}
}
